; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "TouchScreenButton",
"class": "SwipeScreenButton",
"language": "GDScript",
"path": "res://Scenes/TouchScreenButton.gd"
} ]
_global_script_class_icons={
"SwipeScreenButton": ""
}

[application]

config/name="Lightz"
run/main_scene="res://Scenes/StartScreen.tscn"
config/icon="res://Assets/icon.png"

[autoload]

Global="*res://Scenes/Global.tscn"

[display]

window/size/width=720
window/size/height=1280
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

render=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="WorldLayer"
2d_physics/layer_2="BallsLayer"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.0117647, 0.0392157, 0.211765, 1 )
environment/default_environment="res://Scenes/default_env.tres"
